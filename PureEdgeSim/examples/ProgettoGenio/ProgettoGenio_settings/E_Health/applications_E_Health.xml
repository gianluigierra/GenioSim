<applications>
	<application name="E-Health">
		<type>Normal</type>
		<rate>60</rate>
		<usage_percentage>100</usage_percentage>
		<container>
			<container_size>1000000</container_size> <!-- La size del container rappresentata in KB, 1.000.000 = 1GB -->
			<container_request_size>10</container_request_size> <!-- La size della request rappresentata in KB -->
			<shared>false</shared> <!-- se il container deve essere usato da piÃ¹ edge device diversi -->
		</container>
		<task>
			<latency>0.05</latency> <!-- a great number represents no latency-sensitivity -->
			<request_size>10</request_size> <!-- La size della request rappresentata in KB -->
			<results_size>10</results_size> <!-- La size del risultato rappresentato in KB -->
			<task_length>1000</task_length> <!-- la lunghezza del taks rappresentata in Million Istructions (MI) -->
		</task>
	</application>
</applications>

